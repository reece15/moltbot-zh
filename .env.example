# SiliconFlow (Model Provider)
# Get your API key from https://siliconflow.cn/
SILICONFLOW_API_KEY="sk-xxxx"

# Primary Model Selection
# Sets the default model for all agents.
# Common options:
# - deepseek-ai/DeepSeek-V3.2
# - Pro/MiniMaxAI/MiniMax-M2.1
# - Pro/zai-org/GLM-4.7
MOLTBOT_AGENTS_DEFAULTS_PRIMARY_MODEL="deepseek-ai/DeepSeek-V3.2"

# WeCom (Enterprise WeChat) Channel
# Get these from your WeCom admin panel (https://work.weixin.qq.com/)
WECOM_CORPID="ww-your-corp-id"
WECOM_CORPSECRET="your-app-secret"
WECOM_AGENTID="1000001"

# WeCom Message Reception (Webhook)
# These are required to receive messages from WeCom
# URL: http://<your-host>:18789/wecom/webhook
WECOM_TOKEN="your-webhook-token"
WECOM_AESKEY="your-encoding-aes-key"

# Web Search Configuration
# Choose one provider: tavily, brave, or perplexity
WEB_SEARCH_PROVIDER="tavily"

# Option 1: Tavily (https://tavily.com/)
TAVILY_API_KEY="tvly-xxxx"

# Option 2: Brave Search (https://brave.com/search/api/)
# BRAVE_API_KEY="BSAxxxx"

# Option 3: Perplexity (https://www.perplexity.ai/)
# PERPLEXITY_API_KEY="pplx-xxxx"

# Gateway Configuration
# Security token for accessing the web dashboard
CLAWDBOT_GATEWAY_TOKEN="my-secret-token"
